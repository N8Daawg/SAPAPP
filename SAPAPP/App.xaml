<Application x:Class="SAPAPP.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:local="clr-namespace:SAPAPP"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <x:Array x:Key="ProductsList" Type="sys:String">
            <sys:String>Product 1</sys:String>
            <sys:String>Product 2</sys:String>
            <sys:String>Product 3</sys:String>
            <sys:String>Product 4</sys:String>
            <sys:String>Product 5</sys:String>
        </x:Array>

        <x:Array x:Key="PCBList" Type="sys:String">
            <sys:String>PCB 1</sys:String>
            <sys:String>PCB 2</sys:String>
            <sys:String>PCB 3</sys:String>
            <sys:String>PCB 4</sys:String>
            <sys:String>PCB 5</sys:String>
        </x:Array>
    </Application.Resources>
</Application>
